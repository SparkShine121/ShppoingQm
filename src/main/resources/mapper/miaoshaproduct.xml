<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.MiaoshaProductMapper">
    <select id="list" resultType="com.entity.MiaoshaProduct">
        select *
        from miaosha_product
    </select>
    <select id="selectDetailById" resultType="com.entity.MiaoshaProduct">
        select *
        from miaosha_product
        where id = #{id}
    </select>
    <select id="selectList" resultType="com.entity.MiaoshaProduct">
        select *
        from miaosha_product
    </select>

</mapper>